<body>

  <header>
    <!-- Navbar
    ================================================== -->
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <!-- logo -->
          <a class="brand logo" href="/">
            <img src="assets/img/logo.png" alt="" />
          </a>
          <!-- end logo -->

          <!-- top menu -->
          <div>
            <nav>
              <ul class="nav topnav">
                <li class="dropdown success active">
                  <a href="/home"><i class="icon-home icon-white"></i> Home</a>
                </li>
        
        <li class="dropdown warning">
                  <a href="/tournaments"><i class="icon-bullhorn icon-white"></i> Tournaments</a>
               
                </li>
         
        <li class="dropdown success">
                  <a href="/register"><i class="icon-star icon-white"></i>Register</a>
              </li>
               
                <li class="dropdown primary">
                  <a href="/login"><i class="icon-star icon-white"></i>Log in</a>
              </li>
        
                  </ul>
                
               
              </ul>
            </nav>
          </div>
          <!-- end menu -->
        </div>
      </div>
    </div>
  </header>

  <section id="intro">
    <div class="jumbotron masthead">

      <div class="container">
        <div class="row">
          <div class="span12">

            <div id="slider" class="nivoSlider">
               <img src="assets/img/slides/nivo/1.png" data-thumb="assets/img/slides/nivo/1.png" alt="" title="#htmlcaption1" />
              <img src="assets/img/slides/nivo/2.png" data-thumb="assets/img/slides/nivo/2.png" alt="" data-transition="slideInLeft" />
              <img src="assets/img/slides/nivo/3.png" data-thumb="assets/img/slides/nivo/3.png" alt="" title="#htmlcaption2" />              
              <img src="assets/img/slides/nivo/4.png" data-thumb="assets/img/slides/nivo/4.png" alt="" title="#htmlcaption1" />
              <img src="assets/img/slides/nivo/5.png" data-thumb="assets/img/slides/nivo/5.png" alt="" data-transition="slideInLeft" />
              <img src="assets/img/slides/nivo/6.png" data-thumb="assets/img/slides/nivo/6.png" alt="" title="#htmlcaption2" />
              <img src="assets/img/slides/nivo/7.png" data-thumb="assets/img/slides/nivo/7.png" alt="" title="#htmlcaption2" />
            </div>
         

          </div>
        </div>
      </div>
    </div>
  </section>



  <!-- end tagline -->

  <section id="maincontent">
    <div class="container">

      <div class="row">
        <div class="span3">
          <h3 class="heading-success"><span class="btn btn-large btn-success"><i class="m-icon-big-swapright m-icon-white"></i></span>&nbsp;&nbsp;Announcements</h3>
          <p>Announcements for the tournament details in each game and the winner's in each tournament.</p>
        </div>
        <div class="span3">
   

         <div class="span8">
          <div class="well well-success box">
         
          
              <% announcements.forEach(function(element) { %>  
              <h2><%= element.announcementTitle %></h2> <br>
              <p><%= element.announcementMessage %> <hr></p>
              <% }); %>


            
      
    
        </div>
       
        </div>
     
          </div>
        </div>
       
        </div>
  

      <!-- divider -->

      <!-- end divider -->

  </section>


  <section id="bottom">
    <div class="container">

      <!-- divider -->
      <div class="row">
        <div class="span12">
          <div class="divider"></div>
        </div>
      </div>
      <!-- end divider -->


      <div class="row">
        <div class="span12">
          <h3><a class="btn btn-large btn-success" href="Tournaments.html"><i class="m-icon-big-swapdown m-icon-white"></i></a> Upcoming Tournaments</h3>

          <div id="latest-work" class="carousel btleft">
            <div class="row-fluid">
            <ul class="portfolio-area da-thumbs">   
                                <% var jkl = 0;%>   
                                  <% data.forEach(function(element) { %>  
              <li>
        <div class="col">
                <div class="thumbnail">
           <div class="tooltip-demo image-wrapp">
                  <img src="data:img/png; base64, <%=new Buffer(element.Tpic,'binary').toString('base64')%>" alt="">
                  <div class="caption">

                    <h3><%= element.TournamentName.substring(0,11) %></h3>
          <p>No. of participants :  0
            <br> Game:<%= element.TournamentGame %>
            <br> Registration price:  <%= element.registration_fee%>
  
          </p><br> 
                    <p><a data-toggle="modal" href="#myModal<%= jkl %>" class="btn btn-success">More Info</a>
          </div>
                </div><% jkl++ %>
        </div>

              </li>
                              <% }); %>
       </ul>
            </div>
            </div>
          </div>
        </div>
 
  </section>
  
  
<!-- modal content -->
          <div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
              <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              <h3 id="myModalLabel">Tournament Title</h3>
            </div>
            <div class="modal-body">
              <h4>Game: League of Legends</h4>
              <p>No. of participants :  
              <br> Slots Left:
              <br> Registration price:  
              <br> Venue: 
              <br> Date: 
              <br> Time: 
            </p>
        <hr>
              <h4>Bracketing</h4>
          <!-- <p>This <a href="#" role="button" class="btn popover-test" title="A Title" 
          data-content="And here's some amazing content. It's very engaging. right?">button</a> 
          should trigger a popover on click.</p>
            <p><a href="#" class="tooltip-test" title="Tooltip">This link</a> 
            and <a href="#" class="tooltip-test" title="Tooltip">that link</a> should have tooltips on hover.</p>-->
        <hr>
              <h4>Registration</h4>
        
              

              <hr>
            </div>
            <div class="modal-footer">
              <button class="btn" data-dismiss="modal">Close</button>
              <button class="btn btn-success">Register</button>
            </div>
          </div>
 
