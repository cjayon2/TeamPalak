            <form action="/registration" method="post">
				<h5>Tournament:</h5>
				<select name="tourna" required>
                  <option value=""></option>
                                <% if (message.length > 0) { %>
                <div class="alert alert-danger text-center"><%= message %></div>
            <% } %>
                  <% data.forEach(function(element) { %>
                      <option value="<%= element.TournamentID %>"> <%= element.TournamentName %> <%= new Date(element.TSched).toDateString() %></option>
                  <% }); %>
                </select>
				<br>
				<h5>Team Name:</h5>
				<input type="text" placeholder="Name" name="tname" id ="tname" list="tnames" required>
                <datalist id="tnames">
                  <% teams.forEach(function(element) { %>
                      <option value="<%= element.TeamName %>"><%= element.TeamName %></option>
                  <% }); %>
                </datalist>
				<br>
				<br><br>
				<input type="hidden" id="real" name="real">
                <script>
                tname.onchange = function(){
                  changeInput();
                }
                tname.onkeyup = function(){
                  changeInput();
                }
                function changeInput(){
                    console.log("yey");
                  var val_1 = document.getElementById('tname').value;
                  var val_2 = <%= teams[].TeamID %>;
                  document.getElementById('real').value = val_2;
                }
                </script>
